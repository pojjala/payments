        agent any
        
        environment { 
          CC = 'clang'
        }
        stages{
            stage("Example_1"){
              environment { 
                DEBUG_FLAGS = '-g'
              }
              steps{ 
                helloWorld(dayOfWeek:"Monday",name:"OVLN Prasad")
                sh 'printenv'
              }
            }
            stage("Example_2"){
                steps{
                echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
      //              createFolders()    
                }
             }
            
        }
}
