              environment { 
                DEBUG_FLAGS = '-g'
              }
              steps{ 
                helloWorld(dayOfWeek:"Monday",name:"OVLN Prasad")
                sh 'printenv'
              }
            }
            stage("Example_2"){
                steps{
                    createFolders()    
                }
             }
            
        }
}
